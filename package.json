{
  "name": "bty-hub",
  "version": "1.0.0",
  "private": true,
  "description": "BTY-HUB - Modern E-commerce Platform",
  "author": "BTY-HUB Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/bty-hub.git"
  },
  "keywords": [
    "e-commerce",
    "react",
    "shopping",
    "online-store",
    "reactjs",
    "bootstrap",
    "tailwind"
  ],
  "dependencies": {
    "axios": "^1.13.2",
    "bootstrap": "^5.3.8",
    "chart.js": "^4.5.1",
    "core-js": "^3.47.0",
    "date-fns": "^2.30.0",
    "formik": "^2.4.9",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-datepicker": "^6.9.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.30.3",
    "react-scripts": "5.0.1",
    "react-toastify": "^10.0.6",
    "recharts": "^2.15.4",
    "regenerator-runtime": "^0.14.1",
    "web-vitals": "^3.5.2",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.0",
    "sass": "^1.97.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:watch": "react-scripts test --watchAll",
    "test:coverage": "react-scripts test --coverage",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,scss}\"",
    "prebuild": "npm run lint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "analyze": "cross-env ANALYZE=true npm run build",
    "build:analyze": "npm run build && npm run analyze",
    "start:prod": "serve -s build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,scss}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "react-hooks/exhaustive-deps": "warn",
      "react/prop-types": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11",
      "not ie_mob 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "homepage": ".",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
